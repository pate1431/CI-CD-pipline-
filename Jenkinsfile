pipeline{
  agent any
  stages{
    stage("snehal-build-docker-image"){
      steps{
        sh "echo build stage"
        sh "docker --version"
      }
    }
    stage("snehal-login-to-dockerhub"){
      steps{
        sh "echo build stage"
      }
    }
    stage("snehal-push-image-to-dockerhub"){
        steps{
            sh "echo build stage"
        }
    }
  }
}